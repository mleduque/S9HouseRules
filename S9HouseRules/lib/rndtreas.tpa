COPY rndtreas.2da ~override~

OUTER_FOR (i=1;i<~random_treasure_array~;i=i+1) BEGIN
      OUTER_SPRINT ~temp~ $treasure_output(~%i%~)
      APPEND_OUTER ~override/rndtreas.2da~ ~%temp%~
END

